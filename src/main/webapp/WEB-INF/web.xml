<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0"
         metadata-complete="true">

    <display-name>Aplikasi Form</display-name>
    <description>
        Selamat Datang di Aplikasi Form Sederhana
    </description>

    <servlet>
        <servlet-name>FormMahasiswaServlet</servlet-name>
        <servlet-class>service.FormMahasiswaServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>FormMahasiswaServlet</servlet-name>
        <url-pattern>/simpan</url-pattern>
    </servlet-mapping>

</web-app>